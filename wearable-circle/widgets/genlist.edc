group { name: "elm/genlist/item/groupindex/default";

   PROLOG_LIST
   data.item: "texts" "elm.text";

   images {
      image: "tw_list_sub_line_left.#.png" COMP;
      image: "tw_list_sub_line_right.#.png" COMP;
   }

   parts {
      PART_LIST_PADDINGS(
         LIST_GROUPINDEX_PADDING_LEFT_SIZE_INC, LIST_GROUPINDEX_PADDING_RIGHT_SIZE_INC,
         LIST_GROUPINDEX_PADDING_TOP_SIZE_INC, LIST_GROUPINDEX_PADDING_BOTTOM_SIZE_INC
      )
      PART(RECT, "clipper",
         DESC_LR("elm.padding.left.center", "elm.padding.right.center",
         )
      )
      PART(TEXTBLOCK, "elm.text",
         clip_to: "clipper";
         DESC_LRTB("elm.padding.left", "elm.padding.right", "elm.padding.top", "elm.padding.bottom",
            min: 0 LIST_TEXT_GROUPINDEX_HEIGHT_INC;
            PARAM_TEXT("list_text_groupindex_normal",)
         )
      )
      PART(SPACER, "elm.padding.left.center",
         DESC_R("elm.text",
            min: LIST_GROUPINDEX_PADDING_CENTER_SIZE_INC 0;
            max: LIST_GROUPINDEX_PADDING_CENTER_SIZE_INC -1;
            fixed: 1 0;
         )
      )
      PART(SPACER, "elm.padding.right.center",
         DESC_L("elm.text",
            min: LIST_GROUPINDEX_PADDING_CENTER_SIZE_INC 0;
            max: LIST_GROUPINDEX_PADDING_CENTER_SIZE_INC -1;
            fixed: 1 0;
         )
      )
      PART(IMAGE, "left",
         description { state: "default" 0.0;
            min: 0 LIST_GROUPINDEX_SUB_LINE_HEIGHT_INC;
            max: -1 LIST_GROUPINDEX_SUB_LINE_HEIGHT_INC;
            fixed: 0 1;
            align: 0.0 0.5;
            image.normal: "tw_list_sub_line_left.#.png";
            color: 26 64 91 255; /*(Changeable UI) color_class: "B023";*/
            rel1 {
               relative: 0 0;
               to_x: "elm.padding.left";
            }
            rel2 {
               relative: 0 1;
               to_x: "elm.padding.left.center";
            }
         }
      )
      PART(IMAGE, "right",
         description { state: "default" 0.0;
            min: 0 LIST_GROUPINDEX_SUB_LINE_HEIGHT_INC;
            max: -1 LIST_GROUPINDEX_SUB_LINE_HEIGHT_INC;
            fixed: 0 1;
            align: 0.0 0.5;
            image.normal: "tw_list_sub_line_right.#.png";
            color: 26 64 91 255; /*(Changeable UI) color_class: "B023";*/
            rel1 {
               relative: 1 0;
               to_x: "elm.padding.right.center";
            }
            rel2 {
               relative: 1 1;
               to_x: "elm.padding.right";
            }
         }
      )
   }
}

