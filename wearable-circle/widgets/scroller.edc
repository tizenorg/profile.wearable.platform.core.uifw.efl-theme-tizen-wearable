group { name: "elm/genlist/base/focus_bg";
   inherit: "elm/scroller/base/effect";
   alias: "elm/genlist/base/default";
// group { name: "elm/genlist/base/default";
//    alias: "elm/list/base/default";

   images {
      OVERSCROLL_EFFECT_IMAGES
      image: "b_detail_bg_cover.png" COMP;
      image: "b_list_focus_bg.#.png" COMP;
      image: "b_list_focus_bg_ef.#.png" COMP;
   }

   parts {
      part { name: "bg";
         type: RECT;
         description { state: "default" 0.0;
            color: 0 0 0 255;
         }
      }
      part { name: "bg_cover";
         type: IMAGE;
         description { state: "default" 0.0;
            image.normal: "b_detail_bg_cover.png";
            color: 0 0 0 255;
         }
      }
      part { name: "focus_bg";
         type: IMAGE;
         scale: 1;
         mouse_events: 0;
         description { state: "default" 0.0;
            min: 0 130;
            max: -1 130;
            fixed: 0 1;
            align: 0.5 0.5;
            image.normal: "b_list_focus_bg.#.png";
            color: 250 250 250 128;
         }
         description { state: "selected" 0.0;
            inherit: "default" 0.0;
            color: 250 250 250 166;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
            color: 250 250 250 64;
         }
         description { state: "focused" 0.0;
            inherit: "default" 0.0;
            color: 14 144 204 255;
         }
         description { state: "hide" 0.0;
            inherit: "default" 0.0;
            color: 0 0 0 0;
         }
      }
      part { name: "focus_bg_ef";
         type: IMAGE;
         scale: 1;
         mouse_events: 0;
         description { state: "default" 0.0;
            min: 0 130;
            max: -1 130;
            fixed: 0 1;
            align: 0.5 0.5;
            image.normal: "b_list_focus_bg_ef.#.png";
            color: 0 0 0 20;
         }
         description { state: "selected" 0.0;
            inherit: "default" 0.0;
            color: 18 180 255 64;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
            color: 18 180 255 0;
         }
         description { state: "focused" 0.0;
            inherit: "default" 0.0;
            color: 18 180 255 64;
         }
         description { state: "hide" 0.0;
            inherit: "default" 0.0;
            color: 0 0 0 0;
         }
      }
   }
   programs {
      program { name: "selected";
         signal: "elm,state,selected";
         source: "elm";
         action: STATE_SET "selected" 0.0;
         target: "focus_bg";
         target: "focus_bg_ef";
      }
      program { name: "unselected";
         signal: "elm,state,unselected";
         source: "elm";
         action: STATE_SET "default" 0.0;
         target: "focus_bg";
         target: "focus_bg_ef";
      }
      program { name: "enabled";
         signal: "elm,state,enabled";
         source: "elm";
         action: STATE_SET "default" 0.0;
         target: "focus_bg";
         target: "focus_bg_ef";
      }
      program { name: "disabled";
         signal: "elm,state,disabled";
         source: "elm";
         action: STATE_SET "disabled" 0.0;
         target: "focus_bg";
         target: "focus_bg_ef";
      }
      program { name: "focus_bg_show";
         signal: "elm,state,focus_bg,show";
         source: "elm";
         transition: CUBIC_BEZIER 0.3 0 0 1 1;
         action: STATE_SET "default" 0.0;
         target: "focus_bg";
         target: "focus_bg_ef";
      }
      program { name: "focus_bg_hide";
         signal: "elm,state,focus_bg,hide";
         source: "elm";
         transition: CUBIC_BEZIER 0.3 0 0 1 1;
         action: STATE_SET "hide" 0.0;
         target: "focus_bg";
         target: "focus_bg_ef";
      }
   }
}


