         script {
               if (!get_int(rotary_effect_bottom_anim))
                  {
                     set_state(PART:"shadow_bottom", "start", 0.0);
                     set_state(PART:"shadow_bottom_glow", "start", 0.0);
                     set_state(PART:"clipper_bottom", "start", 0.0);
                     set_int(rotary_effect_bottom_anim, 1);
                     run_program(PROGRAM:"edge_bottom_effect_show");
                  }
         }
      }
      program { name: "edge_bottom_effect_show";
